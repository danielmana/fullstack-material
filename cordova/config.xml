<?xml version='1.0' encoding='utf-8'?>
<widget id="net.logzilla.kedb" version="0.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KEDB</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="daniel81bcn@gmail.com" href="https://angel.co/daniel-mana-guixa">
        Daniel Mañá Guixà
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="icon.png" />
    <icon density="ldpi" platform="android" src="icon.png" />
    <icon density="mdpi" platform="android" src="icon.png" />
    <icon density="hdpi" platform="android" src="icon.png" />
    <icon density="xhdpi" platform="android" src="icon.png" />
    <engine name="android" spec="~5.1.0" />
</widget>
